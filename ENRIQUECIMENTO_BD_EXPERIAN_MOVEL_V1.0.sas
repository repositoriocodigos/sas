	ELSE IF   601 <= SCORE_FINAL <=   700 THEN FX_SCORE_SERASA = '601 A  700'; 
	ELSE IF   701 <= SCORE_FINAL <=   800 THEN FX_SCORE_SERASA = '701 A  800';
    ELSE IF   801 <= SCORE_FINAL <=   900 THEN FX_SCORE_SERASA = '801 A  900';  
	ELSE IF   901 <= SCORE_FINAL <=  1000 THEN FX_SCORE_SERASA = '901 A 1000'; 
	ELSE IF          SCORE_FINAL >=  1001 THEN FX_SCORE_SERASA = '>= 1000   ';

RUN;

